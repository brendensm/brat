echo -e "${DARK}░░░░░░░░░░░░░░${NC}${LIME}█${DARK}░░░░░░░░░░░░${NC}${LIME}▀▀█▀▀${NC}${DARK}░░░░░░░░░░░░░░░░░░${NC}"
echo -e "${DARK}░░░░░░░░░░░░░░${NC}${LIME}█▀▀▄${DARK}░${NC}${LIME}█▀▀█${DARK}░${NC}${LIME}▀▀▀█ █${NC}${DARK}░░░░░░░░░░░░░░░░░░░░${NC}"
echo -e "${DARK}░░░░░░░░░░░░░░${NC}${LIME}█${DARK}░░${NC}${LIME}█${DARK}░${NC}${LIME}█${DARK}░░░░${NC}${LIME}█▀▀█${DARK}░${NC}${LIME}█${NC}${DARK}░░░░░░░░░░░░░░░░░░░░${NC}"
echo -e "${DARK}░░░░░░░░░░░░░░${NC}${LIME}█▄▄▀${DARK}░${NC}${LIME}█${DARK}░░░░${NC}${LIME}▀▄▄█${DARK}░${NC}${LIME}█▄▄${NC}${DARK}░░░░░░░░░░░░░░░░░░${NC}"
echo -e "${DARK}░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░${NC}"
